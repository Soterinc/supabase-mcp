{
  "name": "@kavion/mcp-server-supabase",
  "version": "1.0.0",
  "description": "Custom MCP server for Kavion thermal/RGB drone imagery with hybrid SQL and domain-specific tools",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "bin": {
    "mcp-server-kavion": "./dist/transports/stdio.js"
  },
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prebuild": "pnpm typecheck",
    "test": "echo \"No tests yet\""
  },
  "files": ["dist/**/*"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@supabase/mcp-utils": "workspace:^",
    "@supabase/supabase-js": "^2.39.3",
    "common-tags": "^1.8.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.8.6",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}